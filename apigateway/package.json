    "graphql-middleware": "^3.0.2",
    "graphql-tools": "^4.0.4",
    "graphql-type-json": "^0.2.1",
    "graphql-upload": "^8.0.4",
    "jsonwebtoken": "^8.5.0",
    "lodash": "^4.17.11",
